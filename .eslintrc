{
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "es6": true,
        "jest": true
    },
    "extends": ["react-app", "plugin:prettier/recommended"],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2019,
        "sourceType": "module"
    },
    "rules": {
        "jsx-a11y/anchor-is-valid": "off",
        "no-script-url": "off",
        "jsx-a11y/role-supports-aria-props": "off",
        "no-loop-func": "off",
        "array-callback-return": "off"
      }
}
